#include "LCD/LCD.h"
#include "KEYPAD.h"

int main ()
{
u8 pressed  ;
LCD_init();
Keypad_init();

LCD_Send_String("WELCOME %% SLEEM");
_delay_ms(1000);
LCD_Send_Command(LCD_CLEAR_SCREEN);

while (1)
{

    LCD_Send_Character( Keypad_getkey());
	_delay_ms(500);
}
return 0 ;


}
